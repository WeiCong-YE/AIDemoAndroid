<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ai.demo">
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>


    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.ContrastResultActivity">
        </activity>
        <activity android:name=".ui.ConfigActivity">
        </activity>

        <activity android:name=".ui.ContrastOpencvActivity">
        </activity>
        <activity android:name=".ui.OcrActivity"/>
        <activity android:name=".ui.OcrResultActivity"/>
        <activity android:name=".ui.OcrDectResultActivity"/>
        <activity android:name=".ui.OcrDectActivity"/>
        <activity android:name=".ui.NameEntityActivity"/>
    </application>

</manifest>